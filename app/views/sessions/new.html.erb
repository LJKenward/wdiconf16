<div class= "container">
  <div class= "login-container">
    <h1 id="sponsor-header">Admin Login</h1>
    <div class = "tickets-container">
      <%= form_for(:session, url: login_path) do |f| %>
      <ul><li><%= f.label :username %>
      <%= f.text_field :username %></li>
      <li><%= f.label :password %>
      <%= f.password_field :password %></li>
      <li><%= f.submit "Log in" , class: "btn"%></li>
      <% end %>
    </div>
  </div>
</div>